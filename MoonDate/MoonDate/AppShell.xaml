<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MoonDate.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MoonDate"
    Shell.FlyoutBehavior="Disabled" Shell.TabBarBackgroundColor="BlanchedAlmond" Shell.TabBarTitleColor="Black">
    
    

    <TabBar>
        
            <ShellContent
        Title="Main"
                Icon="junk.png"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
        
            <ShellContent
        Title="History"
        ContentTemplate="{DataTemplate local:History}"
        Route="History" />
        
            <ShellContent
        Title="Significance"
        ContentTemplate="{DataTemplate local:Significance}"
        Route="Significance" />
        
    </TabBar>

</Shell>
